{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": ["src/*", "src/*.ios", "src/*.android"],
      "tests": ["tests/*"],
      "components/*": ["src/components/*", "src/components/*.ios", "src/components/*.android"],
      "shared/*": ["src/shared/*", "src/shared/*.ios", "src/shared/*.android"],
      "containers/*": ["src/containers/*"],
      "modules/*": ["src/modules/*"],
      "globals/*": ["src/globals/*"],
      "variables/*": ["src/variables/*"],
      "images/*": ["src/assets/images/*"]
    }
  }
}
